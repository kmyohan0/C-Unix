[ -e test_literal_tests.sh ]
[ -d test_literal_tests.sh ] || echo this_is_correct
[ -f test_literal_tests.sh ]
[-e test_literal_tests.sh]
([ -e test_literal_tests.sh ])
([ -e test_literal_tests.sh ] && echo correct) || echo notWorking
([ -f test_literal_tests.sh ] && echo correct) || echo notWorking
([ -e test_literal_tests.sh ] && echo correct) || (echo this && echo should_not_print)
