test -e test_literal_tests.sh || echo wrong
test -d test_literal_tests.sh || echo wrong
test -f test_literal_tests.sh || echo right_output
(test -e test_literal_tests.sh ) && echo correct
(test -e test_literal_tests.sh && echo hello) || echo notWorking
(test -f test_literal_tests.sh && echo hello) || echo notWorking
(test -e test_literal_tests.sh && echo hello) || (echo this && echo should_not_print)
